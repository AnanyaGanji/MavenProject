aws: JWD9rErR&T3-/G5

<tomcat-users>
  <!-- Add this line inside the <tomcat-users> tag -->
  <user username="jenkins" password="jenkins123" roles="manager-script,admin-gui"/>
</tomcat-users>


pipeline {
    agent any
    tools{
        maven 'MAVEN_HOME'
    }
    stages {
        stage('git repo & clean') {
            steps {
                bat "rmdir /s /q MavenProject || exit 0"
                bat "git clone https://github.com/AnanyaGanji/MavenProject.git"
                bat "mvn clean -f MavenProject/pom.xml"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f MavenProject/pom.xml" 
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f MavenProject/pom.xml"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f MavenProject/pom.xml"
            }
        }
    }
}
